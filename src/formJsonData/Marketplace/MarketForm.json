                },
                {
                    "width": 6,
                    "name": "is_active",
                    "label": "Is Active",
                    "type": "radio",
                    "options": [
                        "Active",
                        "Not Active"
                    ],
                    "useForViewOnly": true
                }
            ]
        }
    ],
