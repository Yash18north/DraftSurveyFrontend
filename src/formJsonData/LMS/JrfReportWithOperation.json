          "moduleType": "JRFOperationAssignment",
          "isViewOnly": "View",
          "headers": [
            {
              "label": "Sample Marks",
              "name": "samples",
              "type": "label",
              "required": true
            },
            {
              "label": "Type",
              "name": "samples",
              "type": "label",
              "required": true
            },
            {
              "label": "Groups of Parameter",
              "name": "Groups",
              "type": "select",
              "required": true,
              "options": [
                "ASTM",
                "IS",
                "ISO"
