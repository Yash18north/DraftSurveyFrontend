          "required": true,
          "value": "To be Generated",
          "characterLimit": 16,
          "readOnly": true,
          "errorMsgs": {
            "required": "This field is required"
          }
        },
        {
          "width": 6,
          "name": "jrf_referenceno",
          "label": "Reference No",
          "type": "text",
          "required": true,
          "readOnly": true
        },
        {
          "width": 6,
          "name": "",
          "label": "",
          "type": "label",
          "value": ""
        },
        {
          "type": "hr"
        },
        {
          "width": 25,
          "label": "Company Details",
          "type": "label",
          "required": true,
          "color": "$danger",
          "textDecoration": "none",
          "styleName": "section_heading",
          "fontSize": "16px"
        },
        {
          "width": 6,
          "name": "jrf_company_name",
          "label": "Organization Name",
          "type": "label",
          "required": true,
          "placeholder": "Enter Name of Organization",
          "errorMsgs": {
            "required": "This field is required"
          },
          "value": "-"
        },
        {
          "width": 6,
          "name": "cmp_address",
          "label": "Organization Address",
          "type": "label",
          "required": true,
          "placeholder": "Enter Organization Address",
          "errorMsgs": {
            "required": "This field is required"
          },
          "value": "-"
        },
        {
          "width": 6,
          "name": "jrf_contact_person",
          "label": "Name of Contact Person",
          "type": "text",
          "required": true,
          "placeholder": "Enter  Name of Contact Person",
          "errorMsgs": {
            "required": "This field is required",
            "pattern": "Please enter Valid name"
          }
        },
        {
          "width": 6,
          "name": "jrf_contact_person_number",
          "label": "Phone No. of Contact Person",
          "tooltip": "Enter a 10-digit phone number (e.g., 1234567890).",
          "type": "phone",
          "required": true,
          "placeholder": "Enter  Number of Contact Person",
          "errorMsgs": {
            "required": "This field is required",
            "pattern": "Please enter Valid Number"
          }
        },
        {
          "type": "hr"
        },
        {
          "width": 25,
          "label": "Commodity Details",
          "type": "label",
          "required": true,
          "color": "$danger",
          "textDecoration": "none",
          "styleName": "section_heading",
          "fontSize": "16px"
        },
        {
          "width": 6,
          "name": "jrf_commodity",
          "label": "Commodity",
          "type": "label",
          "value": "Commodity",
          "required": true,
          "placeholder": "Enter Name of Commodity"
