          "styleName": "custom_status",
          "type": "label"
        },
        {
          "width": 6,
          "name": "jrf_date",
          "label": "Date",
          "type": "date",
          "required": true,
          "placeholder": "Enter Date",
          "pastDate": true,
          "errorMsgs": {
            "required": "This field is required"
          },
          "readOnly": true
        },
        {
          "width": 6,
          "name": "jrf_no",
          "label": "JRF No.",
