        {
          "width": 25,
          "label": "Job Description",
          "type": "label",
          "required": true,
          "color": "$danger",
          "textDecoration": "none",
          "styleName": "section_heading",
          "fontSize": "16px",
          "errorMsgs": {
            "required": "This field is required"
          }
        },
        {
          "width": 6,
          "label": "Description of the sample",
          "name": "jrf_desc_of_sample",
          "type": "textArea",
