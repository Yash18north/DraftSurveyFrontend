{
    "breadcom": [
        {
            "title": "Item Listing",
            "redirect": "/shipment"
        },
        {
            "title": "Shipment Form",
            "redirect": "#"
        }
    ],
    "sections": [
        {
            "moduleType": "ShipmentForm",
            "type": "shipment",
            "title": "Shipment Form",
            "customField": {
                "title": "Export Data",
                "buttons": {
                    "cancel": {
                        "title": "Cancel"
                    },
                    "confirm": {
                        "title": "Save"
                    }
                },
                "fields": []
            },
            "fields": [
                {
                    "width": 25,
                    "label": "Shipment Details",
                    "name": "section_heading",
                    "styleName": "section_heading",
                    "type": "label"
                },
                {
                    "width": 6,
                    "name": "vessel_name",
                    "label": "Vessel Name",
                    "type": "text",
                    "required": true,
                    "placeholder": "Enter Vessel Name"
                },
                {
                    "width": 6,
                    "name": "ship_commodity",
                    "label": "Commodity",
                    "required": true,
                    "type": "DropDownWithLoadMore",
                    "apiendpoint": "/masters/list",
                    "model_name": "commodity",
                    "apimethod": "POST",
                    "isSearchable": true,
                    "optionData": {
                        "id": "cmd_id",
                        "label": "cmd_name"
                    }
                },
                {
                    "width": 6,
                    "name": "ship_sub_commodity",
                    "label": "Sub Commodity",
                    "type": "DropDownWithLoadMore",
                    "apiendpoint": "/ji-commodity/get/",
                    "apimethod": "POST",
                    "isSearchable": "true",
                    "optionData": {
                        "id": "sub_cmd_id",
                        "label": "sub_cmd_name"
                    },
                    "customPayload": {
                        "name": "cmd_id",
                        "value": "fk_commodityid"
                    },
                    "isCustomPayload": true,
                    "required": true,
                    "errorMsgs": {
                        "required": "This field is required"
                    },
                    "value": "-"
                },
                {
                    "width": 6,
                    "name": "ship_total_quantity",
                    "label": "Total Quantity",
                    "type": "number",
                    "required": true,
                    "placeholder": "Enter Total Quantity"
                },
                {
                    "width": 6,
                    "name": "ship_place_of_work",
                    "label": "Place of Work",
                    "type": "select",
                    "required": true,
                    "options": [
                        {
                            "label": "Other",
                            "value": "other"
                        },
                        {
                            "label": "Other Place",
                            "value": "other_place"
                        }
                    ]
                },
                {
                    "width": 6,
                    "name": "ship_appointed_quantity",
                    "label": "Appointed Quantity",
                    "type": "number",
                    "required": true,
                    "placeholder": "Enter App Quantity"
                },
                {
                    "width": 6,
                    "name": "ji_is_loading",
                    "label": "Loading / Unloading",
                    "type": "radio",
                    "options": [
                        "Loading",
                        "Unloading"
                    ],
                    "useForViewOnly": true
                },
                {
                    "width": 6,
                    "fieldWidth": 75,
                    "upperFieldWidth": "75",
                    "name": "loading_unloading_port_name",
                    "secondName": "port_name",
                    "label": "",
                    "type": "doubleText",
                    "secondType": "text",
                    "required": true
                }
            ]
        }
    ],
    "apiEndpoints": {
        "create": "/item-master/create/",
        "update": "/item-master/update/"
    },
    "actions": [
        {
            "label": "bi bi-eye h6",
            "value": "View",
            "type": "icon",
            "redirectUrl": "/itemListing/itemsForm/",
            "permission": "read"
        },
        {
            "label": "bi bi-pencil h6",
            "value": "Edit",
            "type": "icon",
            "redirectUrl": "/itemListing/itemsForm/:EditorId",
            "permission": "update"
        },
        {
            "label": "bi bi-trash h6",
            "value": "Delete",
            "type": "icon",
            "redirectUrl": "/itemListing/itemsForm/",
            "permission": "delete"
        },
        {
            "label": "bi bi-clock-history h6",
            "value": "History",
            "type": "icon",
            "permission": "history"
        }
    ],
    "popupMessages": {
        "download": {
            "headerMsg": "Confirmation!",
            "titleMsg": "Are you sure you want to download?"
        },
        "delete": {
            "headerMsg": "Confirmation!",
            "titleMsg": "Are you sure you want to delete?"
        },
        "post": {
            "headerMsg": "Confirmation!",
            "titleMsg": "Are you sure you want to post?"
        },
        "save": {
            "headerMsg": "Confirmation!",
            "titleMsg": "Are you sure you want to save?"
        },
        "cancel": {
            "headerMsg": "Confirmation!",
            "titleMsg": "Are you sure you want to cancel?"
        }
    }
}