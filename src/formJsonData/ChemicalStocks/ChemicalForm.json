            "fields": [
                {
                    "width": 12,
                    "label": "Add Chemical Stocks",
                    "type": "label",
                    "color": "$danger",
                    "labelDecoration": "none",
                    "styleName": "section_heading",
                    "fontSize": "16px"
                },
                {
                    "label": "Item Code",
                    "name": "fk_item_id",
                    "sortName": "fk_item_id",
                    "type": "DropDownWithLoadMore",
                    "isSearchable": true,
                    "apiendpoint": "/item-master/list/",
                    "model_name": "",
                    "apimethod": "GET",
                    "optionData": {
                        "id": "item_id",
                        "label": "item_rm_code"
                    },
                    "width": 6,
                    "placeholder": "Select Item Code"
                },
                {
                    "width": 6,
                    "name": "stock_fk_lab_id",
                    "label": "Lab",
                    "required": true,
                    "type": "DropDownWithLoadMore",
                    "apiendpoint": "/masters/list",
                    "model_name": "lab",
                    "apimethod": "POST",
                    "isSearchable": true,
                    "optionData": {
                        "id": "lab_id",
                        "label": "lab_name",
                        "label2": "lab_code"
                    }
                },
                {
                    "name": "stock_name_of_chemical",
                    "label": "Name of Chemical Stocks",
                    "type": "text",
                    "width": 6,
                    "placeholder": "Enter name of chemical stocks",
                    "readOnly": true
                },
                {
                    "name": "stock_opening_qty",
                    "label": "Opening Stocks",
                    "type": "number",
                    "width": 6,
                    "required": true,
                    "placeholder": "Enter opening stocks"
                },
                {
                    "name": "stock_purchase_qty",
                    "label": "Purchase Qty",
                    "type": "number",
                    "width": 6,
                    "required": true,
                    "placeholder": "Enter purchase qty"
                },
                {
                    "name": "stock_consumed_qty",
                    "label": "Qty Consumed",
                    "type": "number",
                    "width": 6,
                    "required": true,
                    "placeholder": "Enter qty consumed"
                },
                {
                    "name": "stock_closing_qty",
                    "label": "Closing Stocks",
                    "type": "number",
                    "width": 6,
                    "required": true,
                    "placeholder": "Enter closing stocks"
                },
                {
                    "name": "stock_remark",
                    "label": "Remarks",
                    "type": "text",
                    "width": 6,
                    "required": true,
                    "placeholder": "Enter remarks"
                },
                {
                    "width": 6,
                    "name": "stock_services",
                    "label": "Services",
                    "type": "radio",
                    "options": [
                        "Calibration",
                        "AMC",
                        "Other Services"
                    ],
                    "useForViewOnly": true
                },
                {
                    "name": "stock_expiry_date",
                    "label": "Expiry Date",
                    "type": "date",
                    "width": 6,
                    "required": true,
                    "placeholder": "Enter Expiry Date"
                },
                {
                    "name": "stock_notify_expiry_date",
                    "label": "Expiry Notification Expiry Date",
                    "type": "date",
                    "width": 6,
                    "placeholder": "Enter Notification Expiry Date",
                    "noDefaultValue": true
                },
                {
                    "name": "stock_buffer_value",
                    "label": "Buffer Stock Value",
                    "type": "number",
                    "width": 6,
                    "placeholder": "Enter Buffer Stock Value"
                },
                {
                    "name": "stock_is_show_notification",
                    "label": "Is Show Notification",
                    "type": "radio",
                    "options": [
                        {
                            "value": true,
                            "label": "Yes"
                        },
                        {
                            "value": false,
                            "label": "No"
                        }
                    ],
                    "width": 6,
                    "useForViewOnly": true
                }
            ]
        }
    ],
